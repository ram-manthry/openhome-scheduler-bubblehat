 <!--<h1>Open Home Schedule</h1>!-->
    <div id="wrapper">
        <div id="sidebar-wrapper">

            <img src="img/trademe_logo.png" alt="TradeMe Open Home Scheduler" class="logo">

            

                



            <ul class="sidebar-nav">
                
                <p id="msg"></p>
                {{#each watchlist}}
                    <li class="sidebar-brand">{{title}}<br>
                    <span class="sidebar-brand Location" id="location">{{location}}</span><br>
                    <span style="display:none;" id="geolocation">{{geolocation}}</span><br>
                    

                    {{#each visitTimeList}}
                    <span class="sidebar-brand Time">{{startTime}}</span>
                    {{/each}}
                </li>

                {{/each}}
            </ul>

        </div>

        <div class="map">
            <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">&#9776</a>

            <div id="map-canvas"></div>
        </div>
    </div>
